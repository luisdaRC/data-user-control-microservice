<?xml version="1.0" encoding="UTF-8" ?>

<!-- IntelliJ user, if you see an "Element entity-mappings must be declared", it is a known bug: https://youtrack.jetbrains.com/issue/IDEA-121059 -->

<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd"
                 version="2.1">
    <package>co.edu.unicartagena.control.domain</package>

    <entity class="co.edu.unicartagena.control.domain.entities.Asamblea">
        <table name="asamblea"/>

        <attributes>
            <id name="idAsamblea">
                <column name="idasamblea"/>
            </id>

            <basic name="idPersonalapoyo">
                <column name="personalapoyo_idpa"/>
            </basic>

            <basic name="fechaInicio">
                <column name="fechainicio"/>
            </basic>

            <basic name="fechaFin">
                <column name="fechafin"/>
            </basic>

        </attributes>
    </entity>
</entity-mappings>